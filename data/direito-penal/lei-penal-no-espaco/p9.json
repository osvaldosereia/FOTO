"alternativas": [
{ "letra": "A", "texto": "Somente as proposições I e III estão corretas." },
{ "letra": "B", "texto": "Somente as proposições I e IV estão corretas." },
{ "letra": "C", "texto": "Somente as proposições II e III estão corretas." },
{ "letra": "D", "texto": "Somente as proposições II e IV estão corretas." }
],
"gabarito": "A"
},
